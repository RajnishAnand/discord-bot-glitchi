{
  "name": "discord-bot-glitchi",
  "version": "1.0.0",
  "description": "An open source Discord bot",
  "main": "build/index.js",
  "imports": {
    "#libs": "./build/libs/index.js",
    "#api/@*": "./build/APIs/@*/index.js",
    "#api/*.js": "./build/APIs/*.js"
  },
  "scripts": {
    "build": "tsc -b v14",
    "prestart": "npm build",
    "start": "node build/index.js",
    "dev": "node build/index.js",
    "protected": "PROTECTED=1 npm start",
    "format": "prettier --write --loglevel=warn \"v14/**/*.{js,ts,json}\"",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajnishAnand/discord-bot-glitchi"
  },
  "keywords": [
    "discord",
    "bot",
    "glitchi",
    "node",
    "typescript"
  ],
  "author": "Rajnish Anand",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.52",
    "bad-words": "^3.0.4",
    "cbparser": "^1.0.4",
    "content-type": "^1.0.4",
    "discord.js": "14.15.2",
    "mathjs": "^12.4.2",
    "morse-node": "^0.1.1",
    "node-fetch": "2.7.0",
    "pretty-ms": "^7",
    "skia-canvas": "^1.0.0",
    "typescript": "^5.4.5",
    "vm2": "^3.9.18"
  },
  "devDependencies": {
    "@types/bad-words": "^3.0.1",
    "@types/content-type": "^1.1.5",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.11",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5"
  },
  "lint-staged": {
    "**/*.{ts,md,json}": "prettier --write --ignore-unknown"
  }
}
