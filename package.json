{
  "name": "discord-bot-glitchi",
  "version": "1.0.0",
  "description": "An open source Discord bot",
  "main": "build/index.js",
  "imports": {
    "#libs": "./build/libs/index.js",
    "#api/@*": "./build/APIs/@*/index.js",
    "#api/*.js": "./build/APIs/*.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -b v14",
    "prestart": "yarn build",
    "start": "node build/index.js",
    "dev": "node build/index.js",
    "protected": "PROTECTED=1 npm start",
    "format": "prettier --write --loglevel=warn \"v14/**/*.{js,ts,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RajnishAnand/discord-bot-glitchi"
  },
  "keywords": [
    "discord",
    "bot",
    "glitchi",
    "node",
    "typescript"
  ],
  "author": "Rajnish Anand",
  "license": "ISC",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.40",
    "bad-words": "^3.0.4",
    "cbparser": "^1.0.4",
    "content-type": "^1.0.4",
    "discord.js": "14.9.0",
    "mathjs": "^11.1.0",
    "morse-node": "^0.1.1",
    "node-fetch": "2.6.9",
    "pretty-ms": "7.0.1",
    "skia-canvas": "^1.0.0",
    "typescript": "^5.0.4",
    "vm2": "^3.9.17"
  },
  "devDependencies": {
    "@types/bad-words": "^3.0.1",
    "@types/content-type": "^1.1.5",
    "@types/node": "^18.11.7",
    "@types/node-fetch": "^2.6.2",
    "dotenv": "^16.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "**/*.{ts,md,json}": "prettier --write"
  }
}
